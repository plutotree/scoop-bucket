        "regex": "bookxnotepro-windows-x64-([\\d.]+)-(?<date>\\d+).zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "http://www.bookxnote.com/setup/bookxnotepro-windows-x64-$version-$matchDate.zip"
